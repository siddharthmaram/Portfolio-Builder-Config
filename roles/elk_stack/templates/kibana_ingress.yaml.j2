apiVersion: networking.k8s.io/v1
kind: Ingress
metadata:
    name: {{ kibana_ingress_name }}
    namespace: {{ es_namespace }}
spec:
    rules:
      - host: {{ kibana_host }}
        http: 
          paths:
          - path: /
            pathType: Prefix
            backend:
                service:
                    name: {{ kibana_service_name }}
                    port:
                        number: {{ kibana_port }}